{
  "name": "sysoev.org",
  "version": "1.0.0",
  "description": "Personal blog sysoev.org",
  "author": "Stanislav Sysoev @d4rkr00t",
  "license": "ISC",
  "scripts": {
    "build": "start_time=`date +%s` && gulp prod && gulp perf:critical && cd docpad && ./node_modules/.bin/docpad generate --env production && cd .. && gulp post && echo '==========================================' && echo run time is $(expr `date +%s` - $start_time)s",
    "style": "gulp style:prod && cp ./docpad/src/files/css/style.css ./docpad/out/css/style.css"
  },
  "devDependencies": {
    "browser-sync": "^1.7.2",
    "critical": "^0.5.3",
    "gulp": "^3.8.10",
    "gulp-autoprefixer": "^2.0.0",
    "gulp-browserify": "^0.5.0",
    "gulp-copy": "0.0.2",
    "gulp-html-minifier": "^0.1.6",
    "gulp-load-plugins": "^0.7.1",
    "gulp-minify-css": "^0.3.11",
    "gulp-sass": "^1.1.0",
    "gulp-swig": "^0.7.4",
    "gulp-uglify": "^1.0.2",
    "gulp-uncss": "^0.5.2"
  },
  "dependencies": {
    "dega": "^0.1.3"
  }
}
