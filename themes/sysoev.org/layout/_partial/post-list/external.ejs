<div class="posts-list__item">
    <a href="<%= post.link %>" class="posts-list__item-title title -size-2"><%= post.title %></a>
    <div class="posts-list__item-info">
        <%- partial('_partial/post-list/_date', { postDate: post.date }) %>
        <%- partial('_partial/post-list/_tags-list', { tags: post.tags }) %>
    </div>
    <div class="posts-list__item-desc">
        <%- post.content %>
    </div>
</div>
